{
  "name": "authts",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "nodemon --watch \"./src/**\" --ext \"ts,js\" --ignore \"tests/**/*.test.ts\" --exec \"ts-node src/index.ts\"",
    "predev": "npm run db:reset",
    "test": "mocha -r ts-node/register ./tests/**/*$NAME*.test.ts -t 10000 --exit",
    "lint": "tsc && eslint . --ext .ts",
    "db:reset": "npx -y tsc && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "test:coverage": "nyc --all --include src/models --include src/services --include src/controllers npm run test"
  },
  "keywords": [],
  "author": "Rafael Magalh√£es Guedes",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "express": "^4.18.3",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.2",
    "sequelize": "^6.37.1",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^4.3.12",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mocha": "^10.0.6",
    "@types/sequelize": "^4.28.20",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "chai": "^5.1.0",
    "eslint": "^8.57.0",
    "mocha": "^10.3.0",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.6.2",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
